   "execution_count": 52,
   "id": "6da7fbde-6280-41e4-81f7-6a362dbbc2e1",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      " Recovered bit position from message 'ABCDB': 76\n",
      "\n",
      " Recovered Original Static Key (string): ABCDEFGHIjklmnop\n",
      "HEX   : 4142434445464748496a6b6c6d6e6f70\n",
      "BINARY: 01000001010000100100001101000100010001010100011001000111010010000100100101101010011010110110110001101101011011100110111101110000\n"
     ]
    }
   ],
   "source": [
    "# === Given ===\n",
    "modulated_word = message  # Message that encodes the bit position\n",
    "flipped_key_string_0 = flipped_key_string  # scan from the qr code # Flipped static key in text form\n",
    "\n",
    "# === Step 1: Recover Bit Position from Modulated Word ===\n",
    "bit_position = sum(ord(c) for c in modulated_word) % 128\n",
    "print(f\" Recovered bit position from message '{modulated_word}': {bit_position}\")\n",
    "\n",
    "# === Step 2: Convert Flipped Key String to Bytes ===\n",
    "# Use 'latin1' to preserve 1:1 byte mapping even for non-printable characters like '\\x07'\n",
    "flipped_key_bytes = flipped_key_string_0.encode('latin1')\n",
    "\n",
    "# Sanity check\n",
    "assert len(flipped_key_bytes) == 16, \"Flipped key must be 16 bytes (128-bit)\"\n",
    "\n",
    "# === Step 3: Convert Bytes to 128-bit Binary ===\n",
    "flipped_binary = ''.join(format(byte, '08b') for byte in flipped_key_bytes)\n",
    "\n",
    "# === Step 4: Flip Back the Bit at Known Position ===\n",
    "bit_list = list(flipped_binary)\n",
    "bit_list[bit_position] = '1' if bit_list[bit_position] == '0' else '0'\n",
    "recovered_binary = ''.join(bit_list)\n",
    "\n",
    "# === Step 5: Convert Recovered Binary to Bytes ===\n",
    "recovered_key_bytes = bytes(int(recovered_binary[i:i+8], 2) for i in range(0, 128, 8))\n",
    "\n",
    "# === Step 6: Decode and Show Recovered Original Key ===\n",
    "try:\n",
    "    recovered_key_string = recovered_key_bytes.decode('ascii')\n",
    "    print(f\"\\n Recovered Original Static Key (string): {recovered_key_string}\")\n",
    "except UnicodeDecodeError:\n",
    "       \n",
    "    recovered_key_string = recovered_key_bytes.decode('latin1')\n",
    "\n",
    "print(f\"HEX   : {recovered_key_bytes.hex()}\")\n",
    "print(f\"BINARY: {recovered_binary}\")\n"
   ]
  },
