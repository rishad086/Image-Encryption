      " Bit flipped at position: 76\n",
      "\n",
      " ORIGINAL KEY:\n",
      "ASCII : ABCDEFGHIjklmnop\n",
      "HEX   : 4142434445464748496a6b6c6d6e6f70\n",
      "BINARY: 01000001010000100100001101000100010001010100011001000111010010000100100101101010011010110110110001101101011011100110111101110000\n",
      "\n",
      " FLIPPED KEY:\n",
      "ASCII : ABCDEFGHIbklmnop\n",
      "HEX   : 414243444546474849626b6c6d6e6f70\n",
      "BINARY: 01000001010000100100001101000100010001010100011001000111010010000100100101100010011010110110110001101101011011100110111101110000\n",
      "\n",
      " Modulated Word: ABCDB\n",
      " Check: sum(ord(c)) % 128 = 76\n",
      " Base64 Word: ABCDEFGHIbklmnop\n"
     ]
    }
   ],
   "source": [
    "import random\n",
    "\n",
    "# Step 1: Define a proper 16-character (128-bit) ASCII key\n",
    "key_str = 'ABCDEFGHIjklmnop'  # 16 chars (fixed)\n",
    "original_key = key_str.encode('utf-8')  # bytes\n",
    "\n",
    "assert len(original_key) == 16, f\"Key must be 16 bytes, got {len(original_key)}\"\n",
    "\n",
    "# Step 2: Convert key to binary string\n",
    "binary_key = ''.join(format(byte, '08b') for byte in original_key)\n",
    "\n",
    "# Step 3: Flip a random bit\n",
    "bit_position = random.randint(0, 127)\n",
    "bit_list = list(binary_key)\n",
    "bit_list[bit_position] = '1' if bit_list[bit_position] == '0' else '0'\n",
    "flipped_binary = ''.join(bit_list)\n",
    "\n",
    "# Step 4: Convert flipped binary back to bytes\n",
    "flipped_key_bytes = bytes(int(flipped_binary[i:i+8], 2) for i in range(0, 128, 8))\n",
    "assert len(flipped_key_bytes) == 16\n",
    "\n",
    "# Step 5: Display output\n",
    "print(f\" Bit flipped at position: {bit_position}\")\n",
    "print(f\"\\n ORIGINAL KEY:\")\n",
    "print(f\"ASCII : {original_key.decode('utf-8')}\")\n",
    "print(f\"HEX   : {original_key.hex()}\")\n",
    "print(f\"BINARY: {binary_key}\")\n",
    "\n",
    "print(f\"\\n FLIPPED KEY:\")\n",
    "try:\n",
    "    print(f\"ASCII : {flipped_key_bytes.decode('ascii')}\")\n",
    "except UnicodeDecodeError:\n",
    "    print(\"ASCII : [contains non-printable characters]\")\n",
    "print(f\"HEX   : {flipped_key_bytes.hex()}\")\n",
    "print(f\"BINARY: {flipped_binary}\")\n",
    "\n",
    "# Step 6: Generate a word such that sum(ord(c)) % 128 == bit_position\n",
    "def generate_modulated_word(target_mod, length=5):\n",
    "    base = ord('A')\n",
    "    word = ''\n",
    "    total = 0\n",
    "\n",
    "    for i in range(length - 1):\n",
    "        val = base + (i % 26)\n",
    "        word += chr(val)\n",
    "        total += val\n",
    "\n",
    "    remainder = (target_mod - total % 128) % 128\n",
    "    last_char = chr(remainder if remainder >= 32 else remainder + 32)\n",
    "    word += last_char\n",
    "    return word\n",
    "\n",
    "modulated_word = generate_modulated_word(bit_position)\n",
    "print(f\"\\n Modulated Word: {modulated_word}\")\n",
    "print(f\" Check: sum(ord(c)) % 128 = {sum(ord(c) for c in modulated_word) % 128}\")\n",
    "flipped_key_string = flipped_key_bytes.decode('ascii')\n",
    "print(\" Base64 Word:\", flipped_key_string)"
   ]
