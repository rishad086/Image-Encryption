   "source": [
    "def lsb_embed_data_with_length(image_path, message, output_path):\n",
    "    img = Image.open(image_path).convert(\"RGB\")\n",
    "    pixels = img.load()\n",
    "    width, height = img.size\n",
    "\n",
    "    message_bytes = message.encode('ascii')\n",
    "    message_bits = ''.join(f'{byte:08b}' for byte in message_bytes)\n",
    "    length_bits = f\"{len(message_bytes):016b}\"  # 16-bit length prefix\n",
    "\n",
    "    full_bits = length_bits + message_bits\n",
    "\n",
    "    bit_index = 0\n",
    "    for y in range(height):\n",
    "        for x in range(width):\n",
    "            if bit_index >= len(full_bits):\n",
    "                img.save(output_path)\n",
    "                return output_path\n",
    "\n",
    "            r, g, b = pixels[x, y]\n",
    "            r = (r & 0xFE) | int(full_bits[bit_index])\n",
    "            pixels[x, y] = (r, g, b)\n",
    "            bit_index += 1\n",
    "\n",
    "    img.save(output_path)\n",
    "    return output_path\n"
   ]
  },
