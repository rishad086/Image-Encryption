   "source": [
    "import os\n",
    "import matplotlib.pyplot as plt\n",
    "import json\n",
    "import qrcode\n",
    "import base64\n",
    "from hashlib import sha256\n",
    "from Crypto.Cipher import AES\n",
    "from Crypto.Util.Padding import pad\n",
    "\n",
    "# --- Step 1: Prepare Static Key ---\n",
    "static_key_string = key_str  # Input password (can be 16/24/32 bytes)\n",
    "static_key = sha256(static_key_string.encode()).digest()[:16]\n",
    "print(static_key)# Get 16-byte key via hashing\n",
    "\n",
    "# --- Step 2: Generate Random Dynamic Key ---\n",
    "dynamic_key = os.urandom(16)  # 128-bit random key\n",
    "\n",
    "# --- Step 3: Encrypt Dynamic Key with Static Key ---\n",
    "cipher_static = AES.new(static_key, AES.MODE_ECB)\n",
    "encrypted_dynamic_key = cipher_static.encrypt(pad(dynamic_key, 16))\n",
    "\n",
    "# --- Step 4: Encode keys for QR compatibility ---\n",
    "encoded_encrypted_dynamic_key = base64.b64encode(encrypted_dynamic_key).decode()\n",
    "\n",
    "# --- Step 5: Prepare data for QR code ---\n",
    "qr_data = {\n",
    "    \"static_key\": flipped_key_string, # optional â€“ don't expose whole static key\n",
    "    \"encrypted_dynamic_key\": encoded_encrypted_dynamic_key,\n",
    "    \n",
    "}\n",
    "qr_payload = json.dumps(qr_data)\n",
    "\n",
    "# --- Step 6: Generate QR Code ---\n",
    "qr = qrcode.QRCode(\n",
    "    version=2,\n",
    "    error_correction=qrcode.constants.ERROR_CORRECT_H,\n",
    "    box_size=10,\n",
    "    border=4,\n",
    ")\n",
    "qr.add_data(qr_payload)\n",
    "qr.make(fit=True)\n",
    "\n",
    "img = qr.make_image(fill_color=\"black\", back_color=\"white\")\n",
    "img.save(\"secure_qr.png\")\n",
    "\n",
    "# Display the QR Code\n",
    "plt.figure(figsize=(6, 6))\n",
    "plt.imshow(img, cmap=\"gray\")\n",
    "plt.axis(\"off\")\n",
    "plt.title(\"QR Code with Encrypted Dynamic Key\")\n",
    "plt.show()\n",
    "\n"
   ]
  },
