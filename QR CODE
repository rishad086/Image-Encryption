   "source": [
    "from Crypto.PublicKey import ElGamal\n",
    "from Crypto import Random\n",
    "from Crypto.Util.number import GCD, bytes_to_long, long_to_bytes\n",
    "\n",
    "# Step 1: Generate ElGamal key pair (public + private)\n",
    "def generate_elgamal_keypair(bits=256):\n",
    "    randfunc = Random.new().read\n",
    "    key = ElGamal.generate(bits, randfunc)\n",
    "    return key.publickey(), key"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 33,
   "id": "6d5214fb-19b8-4218-abe0-e41a2a1e2a51",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Step 2: Encrypt the message using ElGamal public key\n",
    "def elgamal_encrypt(message, public_key):\n",
    "    m = bytes_to_long(message.encode())\n",
    "    p = int(public_key.p)\n",
    "    while True:\n",
    "        k = Random.new().read(32)\n",
    "        k_int = int.from_bytes(k, byteorder='big') % (p - 1)\n",
    "        if GCD(k_int, p - 1) == 1:\n",
    "            break\n",
    "    c1 = pow(int(public_key.g), k_int, p)\n",
    "    s = pow(int(public_key.y), k_int, p)\n",
    "    c2 = (m * s) % p\n",
    "    return (c1, c2)"
   ]
  },
