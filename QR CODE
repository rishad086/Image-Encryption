   "id": "40e1ac16-dafe-49d8-907c-4d2525c246d7",
   "metadata": {},
   "outputs": [],
   "source": [
    "def elgamal_decrypt(ciphertext, private_key):\n",
    "    c1, c2 = ciphertext\n",
    "    p = int(private_key.p)\n",
    "    x = int(private_key.x)\n",
    "    s = pow(c1, x, p)\n",
    "    m = (c2 * pow(s, -1, p)) % p\n",
    "    return long_to_bytes(m).decode()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 48,
   "id": "315acdc3-24d2-4758-8026-dc42483abb39",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Decrypted Message: ABCDB\n"
     ]
    }
   ],
   "source": [
    "# Step 1: Extract base64 ciphertext string\n",
    "\n",
    "\n",
    "# Step 2: Decode to original ciphertext (c1, c2)\n",
    "def base64_to_ciphertext(encoded):\n",
    "    data = base64.b64decode(encoded)\n",
    "    c1_bytes, c2_bytes = data.split(b'||')\n",
    "    c1 = int.from_bytes(c1_bytes, byteorder='big')\n",
    "    c2 = int.from_bytes(c2_bytes, byteorder='big')\n",
    "    return (c1, c2)\n",
    "\n",
    "# Step 3: Decrypt\n",
    "# Extract\n",
    "hidden_base64 = lsb_extract_data_with_length(\"qr_with_lsb_hidden_payload.png\")\n",
    "secret_message = base64_to_ciphertext(hidden_base64)\n",
    "decrypted_message = elgamal_decrypt(secret_message, private_key)\n",
    "\n",
    "print(\"Decrypted Message:\", decrypted_message)\n"
   ]
  },
