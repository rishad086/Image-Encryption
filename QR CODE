   "source": [
    "# Convert the ciphertext tuple (c1, c2) to a string (base64 encoded)\n",
    "def ciphertext_to_base64(ciphertext):\n",
    "    c1_bytes = ciphertext[0].to_bytes((ciphertext[0].bit_length() + 7) // 8, byteorder='big')\n",
    "    c2_bytes = ciphertext[1].to_bytes((ciphertext[1].bit_length() + 7) // 8, byteorder='big')\n",
    "    combined = c1_bytes + b'||' + c2_bytes  # custom delimiter\n",
    "    return base64.b64encode(combined).decode()"
   ]
