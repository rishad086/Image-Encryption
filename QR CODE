   "id": "39fada22-b52c-492c-bcb4-bee7b39274d1",
   "metadata": {},
   "outputs": [],
   "source": [
    "def lsb_extract_data_with_length(image_path):\n",
    "    img = Image.open(image_path).convert(\"RGB\")\n",
    "    pixels = img.load()\n",
    "    width, height = img.size\n",
    "\n",
    "    bits = \"\"\n",
    "    for y in range(height):\n",
    "        for x in range(width):\n",
    "            r, g, b = pixels[x, y]\n",
    "            bits += str(r & 1)\n",
    "\n",
    "    # First 16 bits = length\n",
    "    length_bits = bits[:16]\n",
    "    message_length = int(length_bits, 2)\n",
    "\n",
    "    # Extract only message bits\n",
    "    message_bits = bits[16:16 + message_length * 8]\n",
    "    bytes_out = [message_bits[i:i+8] for i in range(0, len(message_bits), 8)]\n",
    "    message = ''.join([chr(int(b, 2)) for b in bytes_out])\n",
    "    return message\n"
   ]
  },
